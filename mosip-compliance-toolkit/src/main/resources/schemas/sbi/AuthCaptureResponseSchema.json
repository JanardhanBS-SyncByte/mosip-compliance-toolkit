{
	"$schema": "https://json-schema.org/draft/2019-09/schema#",
	"$id": "http://mosip-compliance-toolkit.com/schemas/AuthCaptureResponseSchema.json",
	"title": "Auth Capture Response Schema",
	"description": "Auth Capture  Response Schema",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"biometrics": {
			"type": "array",
			"items": [
				{
					"type": "object",
					"properties": {
						"specVersion": {
							"type": "string",
							"enum": [
								"0.9.5",
								"1.0.0"
							]
						},
						"data": {
							"$ref": "#/definitions/non-empty-string"
						},
						"hash": {
							"type": "string"
						},
						"sessionKey": {
							"type": "string"
						},
						"thumbprint": {
							"type": "string"
						},
						"error": {
							"type": "object",
							"properties": {
								"errorCode": {
									"$ref": "#/definitions/non-empty-string",
									"enum": [
										"0"
									]
								},
								"errorInfo": {
									"$ref": "#/definitions/non-empty-string",
									"enum": [
										"Success"
									]
								}
							},
							"required": [
								"errorCode",
								"errorInfo"
							],
							"definitions": {
								"non-empty-string": {
									"type": "string",
									"minLength": 1
								}
							}
						}
					},
					"required": [
						"specVersion",
						"data",
						"hash",
						"sessionKey",
						"thumbprint",
						"error"
					]
				}
			]
		}
	},
	"required": [
		"biometrics"
	],
	"definitions": {
		"non-empty-string": {
			"type": "string",
			"minLength": 1
		}
	}
}